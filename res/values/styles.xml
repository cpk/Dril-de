<resources xmlns:android="http://schemas.android.com/apk/res/android">
 
    
    <style name="ActionBarCompat">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/actionbar_background</item>
    </style>
 
    <style name="DashboardButton">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#515e7b</item>
        <item name="android:background">@drawable/dashboard_btn_event</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:drawablePadding">15dp</item>
		<item name="android:paddingTop">15dp</item>
		<item name="android:paddingBottom">15dp</item>
    </style>    

    
   <style name="FooterBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">#dedede</item>
    </style>
   
   <style name="drilBox">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:paddingRight">8dp</item>
		<item name="android:paddingTop">25dp</item>
		<item name="android:paddingBottom">25dp</item>
		<item name="android:textSize">17sp</item>
		<item name="android:textColor">#000</item>
		<item name="android:layout_margin">4dp</item>
   </style>
   
   <style  name="boxLabel">
       <item name="android:paddingLeft">8dp</item>
       <item name="android:paddingRight">4dp</item>
       <item name="android:paddingTop">10dp</item>
       <item name="android:paddingBottom">0dp</item>
	   <item name="android:textColor">#555555</item>
	   <item name="android:layout_width">match_parent</item>
	   <item name="android:layout_height">wrap_content</item>
   </style>
   
   <style name="rateBtn">
       <item name="android:layout_height">wrap_content</item>
       <item name="android:layout_width">0dp</item>
       <item name="android:layout_weight">1</item>
       <item name="android:layout_margin">1dp</item>
   		<item name="android:textStyle">bold</item>
   		<item name="android:textSize">8sp</item>
   		<item name="android:gravity">bottom|center_horizontal</item>
		<item name="android:paddingTop">40dp</item> 
		
	            
   </style>
   
   <style name="editText">
       <item name="android:paddingTop">15dp</item>
       <item name="android:paddingBottom">15dp</item>
       <item name="android:paddingRight">10dp</item>
       <item name="android:paddingLeft">10dp</item>
       <item name="android:layout_margin">5dp</item>
       <item name="android:layout_width">match_parent</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:inputType">text</item>
       <item name="android:textColor">@color/text_color</item>
   </style>
   
   <style name="editTextMulti">
       <item name="android:paddingTop">5dp</item>
       <item name="android:paddingBottom">5dp</item>
       <item name="android:paddingRight">10dp</item>
       <item name="android:paddingLeft">10dp</item>
       <item name="android:layout_margin">5dp</item>
       <item name="android:layout_width">match_parent</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:minHeight">120dp</item>
       <item name="android:inputType">textMultiLine</item>
       <item name="android:gravity">top|left</item>
       <item name="android:textColor">#333333</item>
       <item name="android:textSize">14sp</item>
       <item name="android:background">@drawable/input</item>
   </style>
   
   <style name="bookAdapter">
       <item name="android:layout_width">match_parent</item>
	   <item name="android:layout_height">wrap_content</item>  
	   <item name="android:paddingLeft">8dp</item>
	   <item name="android:paddingRight">5dp</item>  
   </style>
   
    <style name="ListLabel">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>  
	   <item name="android:textSize">13sp</item>
	   <item name="android:paddingLeft">10dp</item>
	   <item name="android:textColor">@color/text_color</item> 
	   <item name="android:layout_gravity">left|center_vertical</item>   
   </style>
   
    <style name="drilLabel">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>  
	   <item name="android:textSize">11sp</item>
	   <item name="android:paddingLeft">10dp</item>
	   <item name="android:textColor">@color/text_color_label</item> 
	   <item name="android:layout_gravity">center_vertical</item>   
   </style>
   
    <style name="addBtn">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>  
	   <item name="android:layout_gravity">right|center_vertical</item>
	   <item name="android:layout_marginBottom">2dp</item>
	   <item name="android:layout_marginRight">8dp</item> 
	   <item name="android:layout_marginTop">2dp</item>
	   <item name="android:background">@drawable/btn_add</item>
	   <item name="android:height">20dp</item>
	   <item name="android:minWidth">25dp</item>
	   <item name="android:width">25dp</item>   
   </style>
  
    <style name="wordViewBtn">
        <item name="android:layout_weight">0.2</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:gravity">center|bottom</item>
    </style>
    
    <style name="spiker">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>  
       <item name="android:layout_alignParentRight">true</item>
       <item name="android:paddingTop">10dp</item>
       <item name="android:paddingRight">10dp</item>
       <item name="android:paddingLeft">10dp</item>
       <item name="android:layout_marginTop">8dp</item>
       <item name="android:layout_marginRight">8dp</item>
       <item name="android:background">@drawable/speaker_xml</item>
    </style>
 
    <style name="pageLink">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>  
        <item name="android:background">@drawable/link</item>
        <item name="android:padding">15dp</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:text">@color/text_color</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:textColorLink">@color/text_color</item>
    </style>
  
    <style name="tableRow">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>
	   <item name="android:textSize">13sp</item>
	   <item name="android:textColor">@color/text_color</item>  
    </style>
    
    <style name="tableLine">
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#DDDDDD</item>
    </style>
    
    <style name="tableRowWrapper">
       <item name="android:layout_width">wrap_content</item>
	   <item name="android:layout_height">wrap_content</item>
	   <item name="android:padding">5dp</item>
    </style>
    
    <style name="infoTextDescr">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:padding">5dp</item>
	    <item name="android:textSize">12sp</item>
	    <item name="android:textColor">@color/text_color</item> 
    </style>
    <style name="infoWrapper">
         <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:orientation">vertical</item>
	    <item name="android:padding">10dp</item>
	    <item name="android:layout_margin">5dp</item>
	    <item name="android:background">@drawable/table</item>
    </style>
    <style name="infoH1">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:paddingLeft">5dp</item>
	    <item name="android:textSize">15sp</item>
	    <item name="android:textColor">@color/h1</item>
    </style>
    <style name="infoBtn">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:textSize">10sp</item>
        <item name="android:paddingTop">37dp</item>
        <item name="android:clickable">false</item>
    </style>
    <style name="infoBtnLable">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">left|top</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    

</resources>